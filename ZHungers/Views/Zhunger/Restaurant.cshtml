@model IEnumerable<ZHungers.DB.Food_callections>

@{
    ViewBag.Title = "Restaurant";
}

<h2>Restaurant</h2>

<p>
    @Html.ActionLink("Create New", "createCollection")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.r_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.branch)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.food_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.callection_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.e_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.r_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.branch)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.food_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.callection_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.e_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>|
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Logout", "Login")
</div>